{
  "name": "microsoft-todo",
  "version": "2.1.2",
  "author": "woolson <woolson.lee@gmail.com>",
  "description": "macOS client for microsoft todo app",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:win": "node .electron-vue/build.js && electron-builder --win --ia32",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm run lint:fix",
    "asar": "asar list ./build/mac/image-bed.app/Contents/Resources/app.asar"
  },
  "repository": {
    "url": "https://github.com/woolson/microsoft-todo-mac",
    "type": "git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  },
  "build": {
    "productName": "Microsoft ToDo",
    "appId": "com.woolson.todo",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    }
  },
  "dependencies": {
    "deepmerge": "^3.2.0",
    "element-ui": "^2.8.2",
    "fundebug-javascript": "^1.7.3",
    "fundebug-vue": "0.0.1",
    "normalize.css": "^8.0.1",
    "temp-dir": "^2.0.0",
    "vue": "^2.6.10",
    "vue-cal": "^2.3.0",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^8.11.2",
    "vue-line-clamp": "^1.3.2",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-angular": "^7.5.0",
    "asar": "^0.14.3",
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "crc-32": "^1.2.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^4.2.8",
    "electron-builder": "^21.1.5",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-log": "^2.2.17",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "fundebug-nodejs": "^0.1.0",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy": "^1.16.2",
    "husky": "^1.3.1",
    "moment": "^2.22.1",
    "mousetrap": "^1.6.1",
    "msal": "^0.2.4",
    "multispinner": "^0.2.1",
    "nedb": "^1.8.0",
    "network": "^0.4.1",
    "node-loader": "^0.6.0",
    "node-nmap": "^4.0.0",
    "psutils": "^1.0.0",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "update-electron-app": "^1.3.0",
    "url-loader": "^0.5.9",
    "vue-clipboards": "^1.2.4",
    "vue-croppa": "^1.3.6",
    "vue-cropper": "^0.2.9",
    "vue-devtools": "^3.1.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.11.5",
    "webpack-hot-middleware": "^2.18.2"
  }
}
